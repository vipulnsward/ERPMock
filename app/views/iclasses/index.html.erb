<h1>Listing iclasses</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iclasses.each do |iclass| %>
  <tr>
    <td><%= iclass.name %></td>
    <td><%= iclass.desc %></td>
    <td><%= link_to 'Show', iclass %></td>
    <td><%= link_to 'Edit', edit_iclass_path(iclass) %></td>
    <td><%= link_to 'Destroy', iclass, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Iclass', new_iclass_path %>
